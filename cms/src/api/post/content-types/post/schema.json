{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Titulo": {
      "type": "string"
    },
    "conteudo": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "datapublicacao": {
      "type": "datetime"
    },
    "stats": {
      "type": "enumeration",
      "enum": [
        "rascunho",
        "publicado"
      ]
    }
  }
}
